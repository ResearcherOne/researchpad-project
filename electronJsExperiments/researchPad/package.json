{
  "name": "ResearchPad",
  "productName": "ResearchPad",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build": "electron-packager . researchPad --overwrite --platform=win32",
    "build-mac": "electron-packager . researchPad-mac --overwrite --platform=darwin",
    "package": "asar pack build/researchPad-win32-x64/resources/app out/app.asar"
  },
  "keywords": [],
  "author": "researcher1",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "crossref": "^1.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "puppeteer": "^1.13.0",
    "node-fetch": "latest"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.30",
    "@electron-forge/maker-deb": "^6.0.0-beta.30",
    "@electron-forge/maker-rpm": "^6.0.0-beta.30",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.30",
    "@electron-forge/maker-zip": "^6.0.0-beta.30",
    "asar": "^0.14.6",
    "electron": "4.0.1",
    "electron-packager": "^13.0.1"
  }
}
